    "tinacms": "^2.0.0",
    "@tinacms/cli": "^1.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "gh-pages": "^6.3.0",
    "typescript": "^5.6.3"
